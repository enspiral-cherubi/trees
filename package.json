{
  "name": "trees",
  "version": "1.0.0",
  "description": "it makes trees",
  "main": "./src/index.js",
  "scripts": {
    "start": "budo . -d --serve bundle.js --live",
    "build": "browserify . -o bundle.js -g uglifyify",
    "test": "browserify test.js | smokestack | tap-spec",
    "dist": "mkdir -p dist && bash -c 'cp *.{js,html,css} dist'",
    "push": "gh-pages -d dist",
    "deploy": "npm run build && npm run dist && npm run push"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/topological-ragnar/trees"
  },
  "author": "topological-ragnar",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/topological-ragnar/trees/issues"
  },
  "homepage": "https://github.com/topological-ragnar/trees",
  "dependencies": {
    "color": "^0.11.1",
    "concat-stream": "^1.4.11",
    "jquery": "^3.4.0",
    "raf-loop": "^1.1.3",
    "stringstream": "0.0.6",
    "three": "^0.137.0",
    "three-fly-controls": "^1.1.0",
    "three-orbit-controls": "^72.0.0",
    "three-window-resize": "^1.0.4"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.6.0",
    "babelify": "^7.2.0",
    "browserify": "^16.5.0",
    "budo": "^11.6.3",
    "gh-pages": "^2.2.0",
    "smokestack": "^3.6.0",
    "tap-spec": "^5.0.0",
    "tape": "^4.0.0",
    "uglifyify": "^5.0.2",
    "watchify": "^3.11.1"
  }
}
